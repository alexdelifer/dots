# mkinitcpio preset file for the 'linux-xanmod-tt' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-xanmod-tt"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-xanmod-tt.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-xanmod-tt-fallback.img"
fallback_options="-S autodetect"
